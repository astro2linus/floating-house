<% provide(:title, "Floating house") %>
<div class="container">
	<% if current_user %>
		<div class="row">
			<%= current_user.name %>
		</div>
	<% else %>
		<div class="jumbotron text-center">
			<p>Distribute mobile apps over the air</p>
			<%= link_to "Login", login_url, class: "btn btn-warning" %>
		</div>
	<% end %>
</div>



